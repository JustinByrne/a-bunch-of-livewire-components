{
    "name": "justinbyrne/a-bunch-of-livewire-components",
    "description": "As the title suggests it, this is a bunch of Livewire components that can be reused in any Laravel app.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Byrne"
        }
    ],
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^2.10"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "JustinByrne\\ABunchOfLivewireComponents\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustinByrne\\ABunchOfLivewireComponents\\ABunchOfLivewireComponentsServiceProvider"
            ]
        }
    }
}