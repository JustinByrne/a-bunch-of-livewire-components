{
    "name": "justinbyrne/a-bunch-of-livewire-components",
    "description": "As the title suggests it, this is a bunch of Livewire components that can be reused in any Laravel app.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Byrne"
        }
    ],
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^2.10"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "JustinByrne\\ABunchOfLivewireComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustinByrne\\ABunchOfLivewireComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustinByrne\\ABunchOfLivewireComponents\\ABunchOfLivewireComponentsServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}